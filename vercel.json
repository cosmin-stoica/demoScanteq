{
  "version": 2,
  "builds": [
    {
      "src": "api/php/*.php",
      "use": "vercel-php@0.3.6"
    },
    {
      "src": "src/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/php/(.*)",
      "dest": "/api/php/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
